<sdf version='1.7'>
  <world name="default">
    <include>
      <uri>model://red_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>
  </world>

  <!-- Slow down simulation time-->
  <physics name="default_physics" default="true" type="ode">
    <real_time_factor>0.33</real_time_factor>
  </physics>
</sdf>
